@model IEnumerable<Prueba.PROFESOR>

@{
    ViewBag.Title = "Index";
}

<link href="~/www/estilos/CRUDStyle.css" rel="stylesheet" />

<h2>Gestión de Profesores</h2>

<p>
    @Html.ActionLink("Crear", "Create", new { @class = "btn btn-success" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PROF_USU)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PROF_NOMBRE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PROF_APELLIDO)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PROF_CEDULA)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PROF_DIRECCION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PROF_TELF)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PROF_PASSWORD)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TIPO_USUARIO.TU_DESCRIP)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PROF_USU)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PROF_NOMBRE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PROF_APELLIDO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PROF_CEDULA)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PROF_DIRECCION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PROF_TELF)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PROF_PASSWORD)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TIPO_USUARIO.TU_DESCRIP)
        </td>
        <td align="right">
            @Html.ActionLink("Editar", "Edit", new { id = item.ID_PROFESOR }, new { @class = "btn btn-primary" })  |
            @Html.ActionLink("Detalle", "Details", new { id=item.ID_PROFESOR }, new { @class = "btn btn-info" }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.ID_PROFESOR }, new { @class = "btn btn-danger" })
        </td>
    </tr>
}

</table>
